{
  "name": "@sparrowengg/twigs-react",
  "version": "0.37.1",
  "description": "In house component library for SurveySparrow",
  "module": "dist/es/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "jest test",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "rimraf dist && rollup -c",
    "dev": "rimraf dist && rollup -c rollup.config.dev.js --watch",
    "version": "echo $npm_package_version",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "changelog:commit": "git add CHANGELOG.md && git commit -am 'chore: add changelog' && git push",
    "publish-pkg-pr-new": "pkg-pr-new publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:surveysparrow/twigs.git"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "keywords": [
    "react",
    "component-library",
    "ui",
    "design-system",
    "accessible"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://twigs.surveysparrow.com",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@sparrowengg/twigs-react-icons": "*",
    "@storybook/addon-a11y": "^7.0.8",
    "@storybook/addon-actions": "^7.0.8",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/react-webpack5": "^7.0.8",
    "@storybook/testing-library": "0.1.0",
    "@swc/core": "^1.3.93",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-loader": "^8.2.5",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "glob": "^10.3.7",
    "husky": "^4.3.8",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "pkg-pr-new": "^0.0.24",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^6.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-pure": "^0.2.1",
    "storybook": "^7.0.8",
    "ts-jest": "^28.0.8",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@internationalized/date": "^3.2.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
